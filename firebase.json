{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": ".",
    "rewrites": [
      {
        "source": "/admin",
        "function": "admin"
      },
      {
        "source": "/login",
        "function": "login"
      },
      {
        "source": "/sessionLogin",
        "function": "sessionLogin"
      },
      {
        "source": "/sessionLogout",
        "function": "sessionLogout"
      },
      {
        "source": "/sendContactMail",
        "function": "sendContactMail"
      }
    ]
  }
}
